\begin_deeper
\begin_layout Standard

\size footnotesize
A 
\shape italic
balanced growth path equilibrium
\shape default
 consists of (i) the value of product 
\begin_inset Formula $V^{A}(a)$
\end_inset

 and 
\begin_inset Formula $V^{P}$
\end_inset

; (ii) the internal innovation intensity 
\begin_inset Formula $\mu(a)$
\end_inset

; (iii) the distribution of products 
\begin_inset Formula $\hat{F}\left(\hat{z},a\right)$
\end_inset

; (iv) the distribution of matched products 
\begin_inset Formula $\hat{M}_{B}(\hat{z}',a';a)$
\end_inset

 and 
\begin_inset Formula $\hat{M}_{S}(\hat{z}',a';a)$
\end_inset

; (v) the cost function 
\begin_inset Formula $\hat{c}\left(a\right)$
\end_inset

; (vi) the demand shifter for an age 
\begin_inset Formula $a$
\end_inset

 product 
\begin_inset Formula $\hat{D}(a)$
\end_inset

; (vii) the R&D worker wage 
\begin_inset Formula $\hat{w}_{H}$
\end_inset

 (viii) the production worker wage 
\begin_inset Formula $\hat{w}_{L}$
\end_inset

; (ix) the number of variety 
\begin_inset Formula $N$
\end_inset

; (x) the economic growth rate 
\begin_inset Formula $g$
\end_inset

; (xi) and the aggregate output 
\begin_inset Formula $\hat{Y}$
\end_inset

, such that;
\end_layout

\begin_layout Itemize

\size footnotesize
The value of products 
\begin_inset Formula $V^{A}(a)$
\end_inset

 satisfies the HJB
\begin_inset Formula 
\begin{align}
 & \left\{ \rho+\delta_{N}+\left(\theta-1+\beta\left(\sigma-1\right)\right)g\right\} V^{A}(a)\nonumber \\
 & =\frac{1}{\sigma}\left(\tilde{\sigma}\hat{c}(a)\right)^{1-\sigma}\hat{D}(a)+V_{a}^{A}(a)+\mu\left(a\right)\beta\left(\sigma-1\right)V^{A}(a)-\frac{\hat{w}_{H}}{\varphi_{I}}\mu\left(a\right)^{\gamma_{I}}\label{eq:ss_va}
\end{align}

\end_inset

and 
\begin_inset Formula $V^{P}$
\end_inset

 is given by
\begin_inset Formula 
\begin{equation}
V^{P}=\frac{1}{\rho+\delta_{N}+\left(\theta-1\right)g}\frac{\hat{w}_{H}}{\varphi_{E}}\left(1-\frac{1}{\gamma_{X}}\right)\left(\frac{1}{\gamma_{X}}\frac{\varphi_{X}}{\varphi_{E}}\right)^{\frac{1}{\gamma_{X}-1}}\label{eq:ss_vp}
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize

\size footnotesize
The internal innovation intensity 
\begin_inset Formula $\mu(a)$
\end_inset

 satisfies the FOC
\begin_inset Formula 
\begin{equation}
\mu(a)=\left(\frac{\beta\left(\sigma-1\right)\varphi_{I}}{\gamma_{I}\hat{w}_{H}}V^{A}(a)\right)^{\frac{1}{\gamma_{I}-1}}\label{eq:ss_mu}
\end{equation}

\end_inset


\end_layout

\end_deeper
\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
BGP Equilibrium (2)
\end_layout

\end_inset


\end_layout

\begin_layout Frame

\end_layout

\begin_deeper
\begin_layout Itemize

\size footnotesize
The distribution of products 
\begin_inset Formula $\hat{f}\left(\hat{z},a\right)$
\end_inset

 satisfies the KFE
\size scriptsize

\begin_inset Formula 
\begin{equation}
0=-\frac{\partial}{\partial\hat{z}}\left\{ \left(\mu(a)-g\right)\hat{z}\hat{f}\left(\hat{z},a\right)\right\} -\frac{\partial\hat{f}\left(\hat{z},a\right)}{\partial a}+\frac{\delta_{N}}{1-\alpha}\left[\alpha\hat{f}\left(\frac{\hat{z}}{\lambda},a\right)+\left(1-\alpha\right)\hat{\varphi}\left(\hat{z}\right)-\hat{f}\left(\hat{z},a\right)\right]\label{eq:ss_f}
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize

\size footnotesize
The distributions of matched buyers 
\begin_inset Formula $\hat{M}_{B}(\hat{z}',a';a)$
\end_inset

 and sellers 
\begin_inset Formula $\hat{M}_{S}(\hat{z}',a';a)$
\end_inset

 are given by
\end_layout

\begin_layout Standard

\size scriptsize
\begin_inset Formula 
\begin{align}
\hat{M}_{B}(\hat{z}',a';a) & =\zeta_{B}\int_{0}^{\min\left\{ a,a'\right\} }\exp\left(-\left(\delta+\frac{\delta_{N}}{1-\alpha}\right)\tau\right)\hat{F}\left(\exp\left(-\int_{a'-\tau}^{a'}\mu\left(s\right)ds+\tau g\right)\hat{z}',a'-\tau\right)d\tau\label{eq:ss_mb}
\end{align}

\end_inset


\end_layout

\begin_layout Standard

\size scriptsize
\begin_inset Formula 
\begin{equation}
\hat{M}_{S}(\hat{z}',a';a)=\zeta_{S}\int_{0}^{\min\left\{ a,a'\right\} }\exp\left(-\left(\delta+\frac{\delta_{N}}{1-\alpha}\right)\tau\right)\hat{F}\left(\exp\left(-\int_{a'-\tau}^{a'}\mu\left(s\right)ds+\tau g\right)\hat{z}',a'-\tau\right)d\tau\label{eq:ss_ms}
\end{equation}

\end_inset


\end_layout

\end_deeper
