\begin_deeper
\begin_layout Itemize

\size footnotesize
The product cost function 
\begin_inset Formula $\hat{c}\left(a\right)$
\end_inset

 satisfies
\begin_inset Formula 
\begin{align}
\hat{c}\left(a\right)^{1-\sigma} & =\hat{w}_{L}^{\beta\left(1-\sigma\right)}\left(\int\hat{z}'^{\beta\left(\sigma-1\right)}\left(\tilde{\sigma}\hat{c}(a')\right)^{1-\sigma}d\hat{M}_{S}(\hat{z}',a';a)\right)^{1-\beta}\label{eq:ss_c}
\end{align}

\end_inset


\end_layout

\begin_layout Itemize

\size footnotesize
The demand shifter for an age 
\begin_inset Formula $a$
\end_inset

 product 
\begin_inset Formula $\hat{D}(a)$
\end_inset

 satisfies
\begin_inset Formula 
\begin{align}
\hat{D}(a) & =\hat{Y}+\left(1-\beta\right)\tilde{\sigma}^{-\sigma}\hat{w}_{L}^{-\frac{\beta}{1-\beta}\left(\sigma-1\right)}\int\hat{z}'^{\beta\left(\sigma-1\right)}\left(\tilde{c}\left(a'\right)^{1-\sigma}\right)^{-\frac{\beta}{1-\beta}}\hat{D}(a')d\hat{M}_{B}(\hat{z}',a';a)\label{eq:ss_d}
\end{align}

\end_inset


\end_layout

