\begin_inset Formula 
\begin{equation}
\frac{\hat{w}_{H}}{\varphi_{E}}=\alpha\left\{ \lambda^{\beta\left(\sigma-1\right)}V^{A}(0)-\int\hat{z}^{\beta\left(\sigma-1\right)}V^{A}(a)d\hat{F}\left(\hat{z},a\right)\right\} +\left(1-\alpha\right)\left\{ V^{P}+\bar{\epsilon}^{\beta\left(\sigma-1\right)}V^{A}(0)\right\} \label{eq:ss_free_entry}
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize

\size footnotesize
The production worker wage 
\begin_inset Formula $\hat{w}_{L}$
\end_inset

 satisfies the labor market clearing conditions for production workers
\begin_inset Formula 
\begin{equation}
\hat{w}_{L}=\beta\tilde{\sigma}^{-\sigma}N\int\hat{z}^{\beta\left(\sigma-1\right)}\hat{c}(a)^{1-\sigma}\hat{D}(a)d\hat{F}\left(\hat{z},a\right)\label{eq:ss_l_labor}
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize

\size footnotesize
The number of variety 
\begin_inset Formula $N$
\end_inset

 satisfies the labor market clearing conditions for R&D workers
\size scriptsize

\begin_inset Formula 
\begin{equation}
L_{H}=N\left[\frac{1}{\varphi_{I}}\int\hat{z}^{\beta\left(\sigma-1\right)}\mu\left(a\right)^{\gamma_{I}}d\hat{F}(\hat{z},a)+\frac{1}{\varphi_{X}}\left(\frac{1}{\gamma_{X}}\frac{\varphi_{X}}{\varphi_{E}}\right)^{\gamma_{X}/\left(\gamma_{X}-1\right)}+\frac{1}{\varphi_{E}}\left(\frac{\delta_{N}}{1-\alpha}-\left(\frac{1}{\gamma_{X}}\frac{\varphi_{X}}{\varphi_{E}}\right)^{1/\left(\gamma_{X}-1\right)}\right)\right]\label{eq:ss_h_labor}
\end{equation}

\end_inset


